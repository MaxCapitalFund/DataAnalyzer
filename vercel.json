{
  "version": 2,
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/static/index.html"
    },
    {
      "src": "/api/analyze",
      "dest": "/api/test.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
}
