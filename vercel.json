{
  "version": 2,
  "functions": {
    "api/analyze.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/static/index.html"
    },
    {
      "src": "/api/analyze",
      "dest": "/api/test.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
}
