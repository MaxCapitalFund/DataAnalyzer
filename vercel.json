{
  "version": 2,
  "builds": [
    {
      "src": "static/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/test.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
}
